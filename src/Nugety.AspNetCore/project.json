{
  "version": "1.0.1-*",

  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Nugety": "1.0.0-*"
  },


  "frameworks": {
    "net461": {}
  },
  "buildOptions": {    "keyFile": "../../Nugety.snk",
    "xmlDoc": true,
    "nowarn": ["1591"]
  },
  "packOptions": {
    "owners": ["ThinkAbout"],
    "summary": "Nugety.AspNetCore package summary",
    "releaseNotes": "Initial Release",
    "requireLicenseAcceptance": false,
    "tags": ["nugety", "nugetcore.aspnetcore", "nugetyaspnetcore", "c#", "dotnetcore"]
  },

  "scripts": {
    "postcompile": [
      "dotnet pack --no-build --configuration %compile:Configuration% "
    ]
  }
}
